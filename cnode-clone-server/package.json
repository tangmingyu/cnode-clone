{
  "name": "cnode-clone-server",
  "version": "1.0.0",
  "description": "CNode Clone with egg.js",
  "private": true,
  "author": "Tang Mingyu",
  "repository": {
    "type": "git",
    "url": "https://github.com/tangmingyu/cnode-clone.git"
  },
  "egg": {
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-cnode-clone",
    "stop": "egg-scripts stop --title=egg-server-cnode-clone",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.0",
    "egg-mongoose": "^3.1.1",
    "egg-passport": "^2.0.1",
    "egg-passport-local": "^1.2.1",
    "egg-router-plus": "^1.3.0",
    "egg-scripts": "^2.11.0",
    "egg-view-handlebars": "^2.0.1",
    "nodemailer": "^6.2.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "ci": {
    "version": "10"
  },
  "license": "MIT"
}
